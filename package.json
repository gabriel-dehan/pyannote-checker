{
  "name": "pyannote-compare-server",
  "version": "0.0.1",
  "license": "WTFPL",
  "description": "",
  "main": "src/main.ts",
  "_moduleAliases": {
    "src": "./src",
    "config": "./config"
  },
  "engines": {
    "node": ">= 20.x"
  },
  "scripts": {
    "dev": "export NODE_ENV=development; ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/main.ts",
    "dev:db": "docker compose -f docker-compose.yml up -d",
    "dev:db:init": "export NODE_ENV=development; ts-node ./cli/db/init.ts",
    "typeorm": "ts-node -r tsconfig-paths/register -P tsconfig.json ./node_modules/typeorm/cli.js -d config/data-source.ts",
    "build": "node ./build.js",
    "start": "NODE_ENV=production; node ./dist/index.js",
    "workers": "ts-node -r tsconfig-paths/register src/jobs/workers.ts",
    "lint": "yarn lint:path .",
    "lint:path": "eslint --fix --ignore-path .gitignore",
    "prettier": "prettier --write '**/*.{js,ts}'"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^4.0.0",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.13.5",
    "@koa/cors": "^5.0.0",
    "@koa/multer": "^3.0.2",
    "@types/bull": "^4.10.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/koa-logger": "^3.1.5",
    "@types/koa__cors": "^5.0.0",
    "bull": "^4.15.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.4.1",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^7.0.2",
    "koa-jwt": "^4.0.4",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20"
  }
}
